        "@types/lodash": "^4.14.202",
        "@types/react-syntax-highlighter": "^15.5.11",
        "axios": "^1.4.0",
        "browser-image-compression": "^2.0.2",
        "lodash": "^4.17.21",
        "notistack": "^3.0.1",
        "react": "^18.2.0",
        "react-cropper": "^2.3.3",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.1",
        "react-modal": "^3.16.1",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.14.1",
        "react-scripts": "^5.0.1",
        "react-syntax-highlighter": "^15.5.0",
        "recoil": "^0.7.7",
        "styled-components": "^6.0.4",
        "sweetalert": "^2.1.2"
